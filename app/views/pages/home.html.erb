    <script type="text/javascript">
      function initialize() {
        var myLatlng = new google.maps.LatLng(47.5821097, -122.3338412);
        var mapOptions = {
          zoom: 17,
          center: myLatlng
        }
        
        var map = new google.maps.Map(document.getElementById("map-canvas"),
            mapOptions);
        
        var marker = new google.maps.Marker({
            position: myLatlng,
            map: map,
            title: 'Hello World!'
        });
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </head>
  <body>
    <div id="map-canvas"/>
  </body>
